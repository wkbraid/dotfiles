Config {
      position = Bottom

    , template = "%battery% | %wlp2s0wi% }{ %date%"

    , commands =
    [ Run Wireless      "wlp2s0"
                        [ "--template", "Wifi: (<essid>) <quality>"
                        , "--Low",      "30"
                        , "--High",     "70"
                        , "--low",      "red"
                        , "--normal",   "yellow"
                        , "--high",     "darkorange"
                        ] 50

    , Run Battery       [ "--template", "Bat: [<leftbar>]<acstatus>"
                        , "--Low",      "30"
                        , "--High",     "70"
                        , "--low",      "red"
                        , "--normal",   "yellow"
                        , "--high",     "darkorange"
                        , "--"
                            , "-f",     "AC0/online"
                            , "-o",     "<left>%"
                            , "-O",     "<fc=green>Chg</fc>"
                        ] 100

    , Run Date          "<fc=darkorange>%I</fc>:<fc=darkorange>%M</fc> %Y-%m-<fc=darkorange>%d</fc> (%a)" "date" 10
    ]
}
